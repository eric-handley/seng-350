{
  "name": "server",
  "private": true,
  "scripts": {
    "dev": "npx nest start --watch",
    "start": "node dist/main.js",
    "build": "npx nest build",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/database/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/data-source.ts",
    "seed:rooms": "npx ts-node src/database/seeds/seed-rooms.ts",
    "seed:users": "npx ts-node src/database/seeds/seed-demo-users.ts",
    "seed": "npm run seed:rooms && npm run seed:users",
    "db:setup": "npm run migration:run && npm run seed"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express-session": "^1.18.0",
    "@types/supertest": "^2.0.16",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "@auth/core": "^0.37.0",
    "@auth/express": "^0.6.0",
    "@keyv/redis": "^3.0.0",
    "@nestjs/cache-manager": "^2.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.0",
    "cache-manager": "^5.0.0",
    "keyv": "^5.0.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.27",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "bcryptjs": "^2.4.3",
    "express-session": "^1.18.0"
  }
}
